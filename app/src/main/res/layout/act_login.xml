<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/TativoRelativeLayoutPrincipalSinMargen"
    tools:context=".LogIn.Actividades.Act_LogIn"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:touchscreenBlocksFocus="false">
        <Button
            android:id="@+id/frgIniciarSesion"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:backgroundTint="@color/colorAzulOscuro"
            android:textColor="@color/colorBlanco"
            android:text="Iniciar sesion"
            android:textAllCaps="false"/>

        <Button
            android:id="@+id/frgRegistrarse"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:backgroundTint="@color/colorAzulOscuro"
            android:textColor="@color/colorBlanco"
            android:text="Resgistrarse"
            android:textAllCaps="false" />
    </LinearLayout>
    <FrameLayout
        android:id="@+id/flContenedor"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </FrameLayout>
</LinearLayout>
